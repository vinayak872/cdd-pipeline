pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                echo 'Code already checked out'
            }
        }

        stage('Build') {
            steps {
                echo 'Building project'
            }
        }

        stage('Test') {
            steps {
                echo 'Testing project'
            }
        }

        stage('Deploy') {
            steps {
                echo 'Deploying project'
            }
        }
    }
}

